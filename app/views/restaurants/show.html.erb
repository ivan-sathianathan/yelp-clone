<p><%= @restaurant.name %></p>
<p><%= @restaurant.description %></p>
<h1>Reviews for <%= @restaurant.name %>></h1>
  <% if @restaurant.reviews.any? %>
  <ul>
    <% @restaurant.reviews.each do |review| %>
      <li>
        <%= review.thoughts %>, <strong><%= review.rating %></strong>/5
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No reviews.</p>
<% end %>
